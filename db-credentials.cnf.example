# Example:
# user=your_db_user
# password=your_db_password
# host=gateway01.eu-central-1.prod.aws.tidbcloud.com
# port=4000
# ssl-ca=C:\\Users\\prisc\\Desktop\\harvest-hub-clone\\harvest-hub-project\\certs\\db_ca.pem

user=YOUR_DB_USER
password=YOUR_DB_PASSWORD
host=YOUR_DB_HOST
